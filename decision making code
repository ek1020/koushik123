#include<stdio.h> 

 
int main() 
{
    float marks;
    char grade;
 
    printf("Enter marks: ");
    scanf("%f", &marks);
    
   if(marks >= 85 && marks<=100)
    {
        grade = 'A';
    }
    else if(marks >= 70 && marks < 84)
    {
        grade = 'B';
    }
    else if(marks >= 40 && marks < 54)
    {
        grade = 'C';
    }
    else if(marks >= 0 && marks < 40)
    {
        grade = 'D';
    }
    else
    {
    grade = 'F';
    }
    return 0;
    }
